{
  "suggest": {
    "noselect": true
  },
  "languageserver": {
		"haskell": {
			"command": "haskell-language-server-wrapper",
			"args": [
				"--lsp"
			],
			"filetypes": [
				"hs",
				"lhs",
				"haskell"
			]
		},
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
